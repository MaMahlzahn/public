<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>

   <extension
         point="org.eclipse.ui.views">
      <category
            name="PEF Graph"
            id="pdt.pefgraph">
      </category>
      <view
            name="PEF Graph View"
            icon="icons/sample.gif"
            category="pdt.pefgraph"
            class="pdt.pefgraph.internal.views.PEFGraphView"
            id="pdt.pefgraph.views.PEFGraphView">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.eclipse.ui.resourcePerspective">
         <view
               ratio="0.5"
               relative="org.eclipse.ui.views.TaskList"
               relationship="right"
               id="pdt.pefgraph.views.PEFGraphView">
         </view>
      </perspectiveExtension>
   </extension>

</plugin>
