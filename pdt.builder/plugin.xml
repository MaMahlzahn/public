<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.cs3.prolog.connector.bootstrapContribution">
      <fileSearchPathContribution
            alias="pdt_builder_graphml_creator"
            id="pdt.builder.graphml_creator"
            key="yworks.contribution.key"
            path="prolog-src/output-converter/">
         <dependency
               contribution="prolog.library">
         </dependency>
      </fileSearchPathContribution>
   </extension>
   <extension
         point="org.cs3.prolog.connector.bootstrapContribution">
      <fileSearchPathContribution
            alias="pdt_builder_analyzer"
            id="pdt.builder.analyzer"
            path="prolog-src/analyzer/">
      </fileSearchPathContribution>
   </extension>
   <extension
         point="org.cs3.prolog.connector.bootstrapContribution">
      <fileContribution
            id="pdt.builder.fileContribution"
            path="prolog-src/load.pl">
         <dependency
               contribution="prolog.connector.fileContribution">
         </dependency>
      </fileContribution>
   </extension>

</plugin>
