<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="TimeTracker"
   name="TimeTracker Plug-in"
   version="1.0.0"
   provider-name=""
   class="org.cs3.timetracker.TimeTrackerPlugin">

   <runtime>
      <library name="TimeTracker.jar">
         <export name="*"/>
      </library>
   </runtime>

   <requires>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.junit"/>
   </requires>
   <extension
         point="org.eclipse.ui.views">
      <category
            name="Time Tracker"
            id="time.tracker.category"/>
      <view
            class="org.cs3.timetracker.views.TimeTrackerGUIViewer"
            icon="icons/sample.gif"
            category="time.tracker.category"
            name="Time Tracker"
            id="org.cs3.timetracker.TimeTrackerGUIViewer"/>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            name="TimeTicker"
            class="org.cs3.timetracker.TimeTrackerPreferencePage"
            id="org.cs3.timetracker.TimeTrackerPreferencePage">
      </page>
   </extension>

</plugin>
