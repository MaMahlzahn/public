<?xml version='1.0' encoding="ISO-8859-1"?>
<!DOCTYPE session [
	<!ELEMENT session ((reply|request|event|ok)*,exception?)>
	<!ELEMENT ok EMPTY>
	<!ELEMENT reply (tuple*,(failed|exception)?)>
	<!ATTLIST reply
command CDATA #REQUIRED>
	<!ELEMENT request (#PCDATA)>
<!ATTLIST request
type (file|line|char) #REQUIRED
subject CDATA #REQUIRED>
	<!ELEMENT event (#PCDATA)>
<!ATTLIST event
subject CDATA #REQUIRED>
<!ELEMENT tuple (binding*,(failed|exception)?)>
<!ELEMENT binding (#PCDATA)>
<!ATTLIST binding
name	 CDATA #REQUIRED>
<!ELEMENT failed (#PCDATA)>
<!ELEMENT exception (#PCDATA)>
]>
<session><ok/></session>
